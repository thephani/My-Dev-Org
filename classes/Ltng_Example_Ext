public class Ltng_Example_Ext {
    
    Id accountId, caseId;
    
    public CaseAccount_relatedCases_Ext(ApexPages.StandardController sc){
    // comments
        caseId = sc.getId();
    }
    
    public List<Case> getCases(){
        return [SELECT Id, CaseNumber, Subject FROM Case LIMIT 5];
    }

}
